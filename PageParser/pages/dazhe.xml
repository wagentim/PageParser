<?xml version="1.0" encoding="UTF-8"?>
<site>
	<name>DaZhe</name>
	<link>http://dazhe.de/cn</link>
	<id>1</id>
	<block>
		<key>article</key>
		<selector>
			<name>ID</name>
			<result>attr:id</result>
		</selector>
		<selector>
			<name>Product</name>
			<key>header h2 a</key>
			<result>attr:title</result>
		</selector>
		<selector>
			<name>website</name>
			<key>header div.postdata span.category a</key>
			<result>text</result>
		</selector>
		<selector>
			<name>Image Link</name>
			<key>a img</key>
			<result>attr:src</result>
		</selector>
	</block>
</site>