<?xml version="1.0" encoding="UTF-8"?>
<site>
	<name>DaZhe</name>
	<link>http://dazhe.de/cn</link>
	<id>1</id>
	<block>
		<key>article</key>
		<selector def="id">
			<result>attr:id</result>
		</selector>
		<selector def="title">
			<key>header h2 a</key>
			<result>attr:title</result>
		</selector>
		<selector def="site">
			<key>header div.postdata span.category a</key>
			<result>text</result>
		</selector>
		<selector def="img">
			<key>a img</key>
			<result>attr:src</result>
		</selector>
		<selector def="link">
			<key>header div.title h2 a</key>
			<result>attr:href</result>
		</selector>
	</block>
</site>